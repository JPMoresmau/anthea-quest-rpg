{
    "plugins": [
        "react"
    ],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": { "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        } },
    "env": { "es6": true,"jest": true,"browser": true } ,
    "rules":{
        "no-undef": 1,
        "no-unused-vars": 1,
        "no-case-declarations": 0,
        "no-console": 0
    }
}